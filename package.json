{
  "name": "portfoliomanager",
  "version": "0.0.1",
  "description": "Portfolio Manager",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^7",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/sqlite": "^1",
    "@sap/eslint-plugin-cds": "^2.6.5",
    "@sap/ux-specification": "^1.120.4",
    "eslint": "^8.56.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-securities": "cds watch --open securities/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "portfoliomanager.sqlite"
        }
      }
    }
  },
  "sapux": [
    "app/securities"
  ]
}
